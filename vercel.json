{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npx expo export -p web",
          "outputDirectory": "dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ]
  }
  